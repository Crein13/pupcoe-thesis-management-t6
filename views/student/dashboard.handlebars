<div class="container">
	<div class="row">	
		<div class="responsive">
			<div class="container col-sm-12 col-lg-12">
				<table class="all_list" align="center">
				{{#each student}}
				<h1>{{this.first_name}} {{this.last_name}}</h1>
				<h3> {{this.student_number}}</h3>
				<h3> {{this.email}}</h3>
				<h3> {{this.phone}}</h3>
				{{/each}}
			 </div>
		</div>
	</div>
</div>
