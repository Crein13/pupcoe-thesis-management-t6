<div class="container">
	<div class="row">	
		<div class="responsive">
			<div class="container col-lg-12 col-lg-12">		
				<table class="brand_list" align="center">
				<h1 class="text-center" style="margin-bottom:25pt">List of Customers</h1>
				<tr>
					<th>Name</th>
					<th>Email Address</th>
					<th>Customer Details</th>
				</tr>
				{{#each rows}}
				<tr>
					<td>{{this.first_name}} {{this.last_name}}</td>
					<td>{{this.customer_email}}</td>
					<td>
						<a href="/customer/{{this.customer_id}}">See More</a>
					</td>
				{{/each}}
				</table>
			 </div>
		</div>		
	</div>
