<!DOCTYPE html>
<html>
    <body background="{{background}}">
    	<div class="container_member">
    		<div class="col-md-4 col-md-offset-4" align="center">
    			<div class="thumbnail" style="border: 10px solid black; margin-top:7pt">
    				 <img src="{{imageurl}}" alt = "Profile Image" style="width:95%; border: solid 5px; border-radius: 10px ">
    				 <div class="caption">
    				 <h3 class="text-left"><span class="glyphicon glyphicon-user" style="color:#F87217"></span>&nbsp;&nbsp;<strong>{{name}}</strong></h3>
    				 <p class="text-left"><span class="glyphicon glyphicon-envelope" style="color:#F87217"></span>&nbsp;&nbsp;<strong>{{email}}<strong></p>
    				 <p class="text-left"> <span class="glyphicon glyphicon-phone" style="color:#F87217"></span>&nbsp;&nbsp;<strong>{{phone}}<strong></p>
    				 <p class="text-left"><span class="glyphicon glyphicon-sunglasses" style="color:#F87217"></span>&nbsp;&nbsp;<strong>Hobbies</strong></p>
    				 <ul>
                    		{{#each hobbies}}	
							<li>{{this}}</li>
							{{/each}}
                    </ul>	
                 </div>
                </div>
             </div>
         </div>
    </body>
</html>
